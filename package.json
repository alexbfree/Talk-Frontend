{
  "name": "zooniverse-talk",
  "version": "0.0.1",
  "description": "Talk Frontend",
  "main": "src/index.cjsx",
  "scripts": {
    "start": "gulp",
    "test": "jest",
    "deploy-example": "gulp clean && gulp dev-css && gulp webpack-build-once && publisssh ./example demo.zooniverse.org/talk-frontend && gulp clean"
  },
  "browserify": {
    "transform": [
      [
        "coffee-reactify",
        {
          "coffeeout": true
        }
      ],
      "coffeeify"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/marked",
        "<rootDir>/node_modules/moment",
        "<rootDir>/node_modules/emojify.js"
    ],
    "testFileExtensions": [
      "coffee",
      "js",
      "cjsx"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "coffee",
      "cjsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zooniverse/Talk-Frontend.git"
  },
  "author": "Zooniverse",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zooniverse/Talk-Frontend/issues"
  },
  "homepage": "https://github.com/zooniverse/Talk-Frontend",
  "dependencies": {
    "emojify.js": "^0.9.5",
    "marked": "^0.3.2",
    "moment": "^2.8.4",
    "react": "^0.12.1"
  },
  "devDependencies": {
    "browser-sync": "^1.7.0",
    "browserify": "^6.3.2",
    "browserify-shim": "^3.8.0",
    "cjsx-loader": "^1.0.0",
    "coffee-loader": "^0.7.2",
    "coffee-react-transform": "^2.0.0",
    "coffee-reactify": "^2.0.0",
    "coffee-script": "^1.8.0",
    "coffeeify": "^1.0.0",
    "del": "^1.1.0",
    "gulp": "^3.8.10",
    "gulp-stylus": "^1.3.4",
    "gulp-util": "^3.0.1",
    "jest-cli": "^0.1.18",
    "jsx-loader": "^0.12.2",
    "lodash.merge": "^2.4.1",
    "nib": "^1.0.4",
    "publisssh": "^0.2.6",
    "stylus": "^0.49.3",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.1.1",
    "webpack": "^1.4.13"
  },
  "browserify-shim": {
    "react": "global:React"
  }
}
