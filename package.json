{
  "name": "zooniverse-talk",
  "version": "0.0.1",
  "description": "Talk Frontend",
  "main": "src/index.cjsx",
  "scripts": {
    "start": "gulp",
    "test": "jest"
  },
  "browserify": {
    "transform": [
      [
